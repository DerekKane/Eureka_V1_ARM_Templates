{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "String"
        },
        "location": {
            "type": "String"
        },
        "admins": {
            "type": "Array"
        },
        "enableAutomaticBillingUnitAdjustment": {
            "type": "Bool"
        },
        "sku": {
            "type": "String"
        },
        "users": {
            "type": "Array"
        }
    },
    "resources": [
        {
            "type": "Microsoft.DataCatalog/catalogs",
            "name": "[parameters('name')]",
            "apiVersion": "2015-07-01-preview",
            "location": "[parameters('location')]",
            "properties": {
                "admins": "[parameters('admins')]",
                "users": "[parameters('users')]",
                "sku": "[parameters('sku')]",
                "enableAutomaticBillingUnitAdjustment": "[parameters('enableAutomaticBillingUnitAdjustment')]"
            }
        }
    ]
}